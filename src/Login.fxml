            <Label layoutX="72.0" layoutY="131.0" text="Login" />
            <Label layoutX="71.0" layoutY="211.0" text="Hasło" />
            <PasswordField fx:id="txt_password" layoutX="165.0" layoutY="214.0" promptText="hasło" />
            <TextField fx:id="txt_login" layoutX="165.0" layoutY="134.0" promptText="login" />
            <ImageView fx:id="imgV_lock" fitHeight="58.0" fitWidth="56.0" layoutX="101.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
